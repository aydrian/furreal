{
  "name": "furreal",
  "version": "1.0.0",
  "description": "A daily picture application for pets similar to BeReal.",
  "private": true,
  "sideEffects": false,
  "author": "Aydrian Howard <aydrian@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "@remix-run/node": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@remix-run/vercel": "^1.9.0",
    "@vercel/node": "^2.6.2",
    "bcryptjs": "^2.4.3",
    "data-uri-to-buffer": "^3.0.1",
    "date-fns": "^2.29.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-webcam": "^7.0.1",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@remix-run/serve": "^1.9.0",
    "@tailwindcss/typography": "^0.5.8",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "^8.27.0",
    "postcss": "^8.4.20",
    "prisma": "^4.8.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
